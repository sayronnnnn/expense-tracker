# Copy to .env and set values. For Docker, only JWT_SECRET is required here;
# MONGODB_URL is overridden by docker-compose for the API container.

# Required
MONGODB_URL=mongodb://localhost:27019
JWT_SECRET=your-secret-key-change-in-production

# Optional (defaults shown)
MONGODB_DB_NAME=expense_tracker
CORS_ORIGINS=http://localhost:5173
DEBUG=false
ENVIRONMENT=development
